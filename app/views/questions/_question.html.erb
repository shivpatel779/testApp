<td><%=question.pri%></td>
<td><%=question.question%></td>
<td><%=question.teaming_stages%></td>
<td><%=question.appears_day%></td>
<td><%=question.frequency%></td>
<td><%=question.question_type%></td>
<td><%=question.required?  %></td>
<td><%=question.conditions %></td>
<td><%=question.mapping.name %></td>
<% if current_user.has_role? :admin %>
  <td>
    <%= link_to 'Edit', edit_question_path(question), remote: true %>
    <%= link_to 'Destroy', question, method: :delete, data: { confirm: 'Are you sure?' } %>
  </td>
<% end %>